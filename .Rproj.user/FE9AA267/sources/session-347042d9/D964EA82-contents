#!/usr/bin/env python
# coding: utf-8

# In[1]:


import os
 
os.chdir('/Users/mosesowusu/Desktop/Fall 2022/UTRGV thesis/Test/1 NSR')
print(os.getcwd())
 
for count, f in enumerate(os.listdir()):
    f_name, f_ext = os.path.splitext(f)
    f_name = "C" + str(count)
 
    new_name = f'{f_name}{f_ext}'
    os.rename(f, new_name)


